<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
      <add key="sb:uri" value="sb://[ASB namespace here].servicebus.windows.net"/>
      
      <!-- Enter the Shared Access Signature information below to connect to your Azure Service Bus instance -->
      <add key="sb:sas:keyName" value="[key name]"/>
      <add key="sb:sas:sharedAccessKey" value="[key value]"/>
    </appSettings>
</configuration>
